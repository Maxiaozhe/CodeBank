 name='AdvFormula'><code event='defaultvalue'><formula>"SELECT @All"</formula></code><code
 event='inputtranslation'><formula>@If( $QueryType = "all"; @Return( "Select @All"); $QueryType = "adv"; @Return( AdvFormula); Continue);

tAuthStr :="\"" + @Implode( @Name([Canonicalize]; ProfileAuthors); "\":\"") + "\"";
tAuth := @If( AuthorMatch = "1"; "@contains(@lowercase(from); " + @LowerCase(tAuthStr) + ")"; "");

tSubjStr := "\"" + @Implode( @LowerCase(ProfileStrings); "\":\"") + "\"";
tSubj := @If( SubjectMatch = "1";
@If( SubjectOp = "!cont"; "!" ; "") +@If( SubjectOp = "cont" : "!cont";  "@Contains(@LowerCase(Subject); " +tSubjStr + ")";
@If( SubjectOp = "beg" ; "@Begins( "; "@Ends( ") + "Subject; " + tSubjStr + ")"); "");

tCatStr := "\"" + @Implode( @LowerCase(ProfileCategories); "\":\"") + "\"";
tCat := @If(CategoryMatch = "1";  "@Contains(@LowerCase(Categories); " +tCatStr + ")"; "");

tThrMStr := "\"" + @Implode( @LowerCase(ProfileThreadSubjects); "\":\"") + "\"";
tThrM := @If(ThreadMatch = "1";  "@LowerCase(Subject) = " +tThrMStr; "");

tThrRStr := @Implode(ProfileStatus; ":") ;
tThrR := @If(StatusMatch = "1";  "Status = " + tThrRStr ; "");

tQuery := @Implode( @Trim( tAuth : tSubj : tCat : tThrM : tThrR);
@If( AndOr = "and" ; " &amp; "; " | "));
"SELECT " + @If( @Length( tQuery) = 0; "@All"; tQuery)</formula></code></field></run>  <button
